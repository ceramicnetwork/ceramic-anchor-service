{
   "name": "ceramic-anchor-service",
   "version": "0.0.2",
   "author": "Janko Simonovic <simonovic86@gmail.com>",
   "license": "MIT",
   "keywords": [
      "ceramic",
      "anchor",
      "eth",
      "3box"
   ],
   "scripts": {
      "test": "./node_modules/.bin/jest --forceExit --detectOpenHandles --coverage --runInBand",
      "coverage": "nyc npm run test",
      "build": "./node_modules/.bin/tsc -p tsconfig.json",
      "format": "prettier --write 'src/**/*{.ts,.tsx,.js}'",
      "prepublishOnly": "npm run build",
      "prebuild": "npm run clean",
      "postinstall": "node-config-ts",
      "lint": "./node_modules/.bin/eslint --fix ./src --ext .js,.jsx,.ts,.tsx",
      "typeorm": "ts-node -r tsconfig-paths/register ./node_modules//typeorm/cli.js",
      "clean": "rm -rf ./build; rm -rf coverage; rm -rf .nyc_output",
      "start": "node ./build/app.js"
   },
   "husky": {
      "hooks": {
         "pre-commit": "npm run lint",
         "pre-push": "npm test"
      }
   },
   "dependencies": {
      "@overnightjs/core": "^1.7.4",
      "@overnightjs/logger": "^1.1.12",
      "colors": "^1.3.3",
      "config.ts": "^1.0.0",
      "cors": "^2.8.5",
      "cron-parser": "^2.15.0",
      "ejs": "^3.1.3",
      "ethers": "^5.0.7",
      "ethers-typescript-typings": "0.0.4",
      "express": "^4.17.1",
      "express-async-handler": "^1.1.4",
      "express-promise-router": "^4.0.1",
      "express-validator": "^6.6.0",
      "globby": "^11.0.1",
      "http-status-codes": "^1.3.2",
      "ipfs-http-client": "^45.0.0",
      "multihashes": "^1.0.1",
      "node-cron": "^2.0.3",
      "reflect-metadata": "^0.1.10",
      "simple-color-print": "^1.0.4",
      "typeorm": "^0.2.25",
      "typestub-multibase": "^0.0.5",
      "typestub-multihashes": "^0.0.4",
      "uuid": "^8.2.0",
      "pg": "^8.3.0",
      "dotenv": "^8.2.0",
      "os-utils": "latest"
   },
   "devDependencies": {
      "@babel/core": "^7.9.0",
      "@babel/preset-env": "^7.8.4",
      "@babel/preset-typescript": "^7.9.0",
      "@types/cors": "^2.8.6",
      "@types/dotenv": "^8.2.0",
      "@types/events": "^3.0.0",
      "@types/express": "^4.17.4",
      "@types/ganache-core": "^2.7.0",
      "@types/jest": "^25.2.1",
      "@types/node": "^13.11.0",
      "@types/node-cron": "^2.0.3",
      "@types/os-utils": "0.0.0",
      "@types/uuid": "^7.0.2",
      "@typescript-eslint/eslint-plugin": "^2.19.0",
      "@typescript-eslint/parser": "^2.19.0",
      "babel-jest": "^25.1.0",
      "eslint": "^6.8.0",
      "eslint-plugin-jest": "^23.8.2",
      "ganache-core": "^2.10.2",
      "husky": "^4.2.5",
      "ipfs": "^0.41.2",
      "jest": "^25.3.0",
      "node-config-ts": "^3.0.4",
      "nyc": "^15.0.1",
      "prettier": "^2.0.2",
      "sqlite3": "^4.1.1",
      "tmp-promise": "^2.0.2",
      "ts-jest": "^25.4.0",
      "ts-node": "^8.8.1",
      "tsconfig-paths": "^3.9.0",
      "typescript": "^3.8.3"
   }
}
