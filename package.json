{
   "name": "ceramic-anchor-service",
   "version": "0.0.2",
   "author": "Janko Simonovic <simonovic86@gmail.com>",
   "license": "(Apache-2.0 OR MIT)",
   "keywords": [
      "ceramic",
      "anchor",
      "eth",
      "3box"
   ],
   "scripts": {
      "test": "./node_modules/.bin/jest --forceExit --detectOpenHandles --coverage --runInBand",
      "coverage": "nyc npm run test",
      "build": "./node_modules/.bin/tsc -p tsconfig.json",
      "format": "prettier --write 'src/**/*{.ts,.tsx,.js}'",
      "prepublishOnly": "npm run build",
      "prebuild": "npm run clean",
      "postinstall": "node-config-ts",
      "lint": "./node_modules/.bin/eslint --fix ./src --ext .js,.jsx,.ts,.tsx",
      "typeorm": "ts-node -r tsconfig-paths/register ./node_modules//typeorm/cli.js",
      "clean": "rm -rf ./build; rm -rf coverage; rm -rf .nyc_output",
      "start": "node ./build/app.js",
      "startDev": "NODE_ENV=dev APP_MODE=bundled node ./build/app.js ",
      "watch": "nodemon -e ts --watch src --exec \"npm run build && npm run start\""
   },
   "husky": {
      "hooks": {
         "pre-commit": "npm run lint"
      }
   },
   "dependencies": {
      "@ceramicnetwork/3id-did-resolver": "^0.5.3",
      "@ceramicnetwork/common": "^0.14.0",
      "@ceramicnetwork/docid": "^0.3.0",
      "@ceramicnetwork/http-client": "^0.8.4",
      "@ceramicnetwork/key-did-resolver": "^0.2.0",
      "@overnightjs/core": "^1.7.5",
      "@overnightjs/logger": "^1.2.0",
      "base64url": "^3.0.1",
      "colors": "^1.3.3",
      "config.ts": "^1.0.0",
      "cors": "^2.8.5",
      "dag-jose": "^0.3.0",
      "dids": "^1.1.0",
      "dotenv": "^8.2.0",
      "ejs": "^3.1.5",
      "ethers": "^5.0.23",
      "ethers-typescript-typings": "0.0.4",
      "express": "^4.17.1",
      "express-async-handler": "^1.1.4",
      "express-promise-router": "^4.0.1",
      "express-validator": "^6.6.1",
      "globby": "^11.0.1",
      "http-status-codes": "^2.1.4",
      "ipfs-core": "^0.3.0",
      "ipfs-http-client": "^48.1.2",
      "lru-cache": "^6.0.0",
      "morgan": "^1.10.0",
      "multiformats": "^3.0.3",
      "multihashes": "^3.0.1",
      "os-utils": "^0.0.14",
      "pg": "^8.5.1",
      "reflect-metadata": "^0.1.10",
      "rotating-file-stream": "^2.1.3",
      "simple-color-print": "^1.0.4",
      "sqlite3": "^4.2.0",
      "tsyringe": "^4.4.0",
      "typeorm": "^0.2.29",
      "typeorm-transactional-cls-hooked": "0.1.12",
      "typestub-multibase": "^0.0.5",
      "typestub-multihashes": "^0.0.4",
      "uuid": "^8.3.1",
      "aws-cron-parser": "1.1.5"
   },
   "devDependencies": {
      "@babel/core": "^7.12.9",
      "@babel/plugin-proposal-decorators": "^7.12.1",
      "@babel/plugin-transform-modules-commonjs": "^7.12.1",
      "@babel/preset-env": "^7.12.7",
      "@babel/preset-typescript": "^7.12.7",
      "@types/cors": "^2.8.8",
      "@types/dotenv": "^8.2.0",
      "@types/events": "^3.0.0",
      "@types/express": "^4.17.9",
      "@types/ganache-core": "^2.7.0",
      "@types/jest": "^26.0.16",
      "@types/morgan": "^1.9.2",
      "@types/node": "^12.19.8",
      "@types/os-utils": "0.0.1",
      "@types/uuid": "^8.3.0",
      "@typescript-eslint/eslint-plugin": "^4.9.0",
      "@typescript-eslint/parser": "^4.9.0",
      "babel-jest": "^26.6.3",
      "eslint": "^7.14.0",
      "eslint-plugin-jest": "^24.1.3",
      "ganache-core": "^2.13.1",
      "husky": "^4.3.0",
      "jest": "^26.6.3",
      "node-config-ts": "^3.1.0",
      "nodemon": "^2.0.6",
      "nyc": "^15.1.0",
      "prettier": "^2.2.1",
      "tmp-promise": "^3.0.2",
      "ts-jest": "^26.4.4",
      "ts-node": "^9.0.0",
      "tsconfig-paths": "^3.9.0",
      "typescript": "^4.1.2"
   }
}
